{
  "name": "study-assist",
  "version": "0.1.0",
  "description": "A browser extension to intelligently organise your bookmarks",
  "contributors": [
    "Elisabeth Vonesch",
    "Vincent Reynaud <mail@vincentreynaud.de> (https://vincentreynaud.de)"
  ],
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "now-build": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:firefox": "web-ext run --source-dir ./public/",
    "start:popup": "npm run build && web-ext run --source-dir ./build/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "jquery": "^3.4.1",
    "lodash.tonumber": "^4.0.3",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "reactstrap": "^8.0.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "node-sass-chokidar": "^1.3.5",
    "web-ext": "^3.0.0",
    "webextension-toolbox": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
