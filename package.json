{
  "name": "study-assist",
  "version": "0.1.0",
  "description": "A browser extension to help you organise your bookmarks intelligently",
  "contributors": [
    "Elisabeth Vonesch",
    "Vincent Reynaud <mail@vincentreynaud.de> (https://vincentreynaud.de)"
  ],
  "private": false,
  "main": "public/background.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all -p build:**",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "now-build": "npm run build",
    "dev:chrome": "webextension-toolbox dev chrome --devtool=true",
    "dev:firefox": "webextension-toolbox dev firefox --devtool=true",
    "build:chrome": "webextension-toolbox build chrome",
    "build:firefox": "webextension-toolbox build firefox"
  },
  "dependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "jquery": "^3.4.1",
    "lodash.tonumber": "^4.0.3",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "reactstrap": "^8.0.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "svg-inline-loader": "^0.8.0",
    "webextension-toolbox": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
